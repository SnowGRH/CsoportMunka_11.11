<script>
    P.when('A','jQuery','ready').execute(function (A, $) {

        let logCount = function (customTag, count) {
            let ue = window.ue;
            if (count === null || isNaN(count)) {
                count = 1;
            }
            customTag = customTag.replace(/\s/g,"");
            if (ue && ue.count){
                ue.count(customTag, count);
            }
        }

        let hbrOptInCheckBoxInputElements = document.querySelectorAll("#hbrOptInCheckBox")
        hbrOptInCheckBoxInputElements.forEach(element => element.addEventListener("click", enrollHbrProgram))

        let hbrOptInOfferMetric = "BNXDP.HbrOptIn." + '';

        function enrollHbrProgram(e) {
            if (e.target.checked) {
                let hbrCheckBoxes = document.querySelectorAll(".hbr-non-enrolled")
                let enrolledStateTextAreas = document.querySelectorAll(".hbr-enrolled-on-check")

                let optInUrl = ''
                let successOptInMetric = hbrOptInOfferMetric + ".Success"
                let failureOptInMetric = hbrOptInOfferMetric + ".Failure"

                if(optInUrl) {
                    A.ajax(optInUrl, {
                        method:'get',
                        paramsFormat:'string',
                        params:'',
                        success: function(data) {
                            if (data.includes('FAILURE') || data.includes('EXCEEDED')) {
                                hbrOptInCheckBoxInputElements.forEach(function () {
                                    this.checked = false;
                                });
                                logCount(failureOptInMetric, 1);
                                alert('');
                                return;
                            }
                            hbrCheckBoxes.forEach(element => element.style.display = "none")
                            enrolledStateTextAreas.forEach(element => element.style.display = "inherit")
                            logCount(successOptInMetric, 1);
                        },
                        error: function() {
                            hbrOptInCheckBoxInputElements.forEach(function () {
                                this.checked = false;
                            });
                            logCount(failureOptInMetric, 1);
                            alert('');
                        }
                    });
                }
            }
        }
    })
</script>
